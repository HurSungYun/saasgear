{
	"name": "api",
	"version": "1.0.0",
	"main": "server.js",
	"license": "MIT",
	"type": "module",
	"scripts": {
		"start": "nodemon --inspect server",
		"db:migrate": "knex migrate:latest --knexfile constants/knexfile.cjs",
		"db:rollback": "knex migrate:rollback --knexfile constants/knexfile.cjs",
		"db:seed": "knex --esm seed:run --knexfile constants/knexfile.cjs"
	},
	"dependencies": {
		"apollo-server-express": "^2.19.0",
		"dotenv": "^8.2.0",
		"express": "^4.17.1",
		"knex": "^0.21.12",
		"morgan": "^1.10.0",
		"mysql2": "^2.2.5",
		"redis": "^3.0.2"
	},
	"devDependencies": {
		"nodemon": "^2.0.6"
	}
}
